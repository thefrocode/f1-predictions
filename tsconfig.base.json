{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2015",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@f1-predictions/add-league-dialog": [
        "libs/leagues/ui/add-league-dialog/src/index.ts"
      ],
      "@f1-predictions/app-config": ["libs/shared/app-config/src/index.ts"],
      "@f1-predictions/auth-store": ["libs/auth/store/src/index.ts"],
      "@f1-predictions/auth/feature/login": [
        "libs/auth/feature/login/src/index.ts"
      ],
      "@f1-predictions/callback": ["libs/auth/feature/callback/src/index.ts"],
      "@f1-predictions/f1-predictions-api": [
        "libs/shared/data-access/f1-predictions-api/src/index.ts"
      ],
      "@f1-predictions/home": ["libs/home/feature/src/index.ts"],
      "@f1-predictions/home-session-times": [
        "libs/home/ui/home-session-times/src/index.ts"
      ],
      "@f1-predictions/home-teamlock-deadline": [
        "libs/home/ui/home-teamlock-deadline/src/index.ts"
      ],
      "@f1-predictions/join-team-dialog": [
        "libs/leagues/ui/join-league-dialog/src/index.ts"
      ],
      "@f1-predictions/layout": ["libs/shell/ui/layout/src/index.ts"],
      "@f1-predictions/league-players-list": [
        "libs/leagues/features/league-players-list/src/index.ts"
      ],
      "@f1-predictions/league-settings": [
        "libs/leagues/features/league-settings/src/index.ts"
      ],
      "@f1-predictions/leagues-add": [
        "libs/leagues/features/leagues-add/src/index.ts"
      ],
      "@f1-predictions/leagues-join": [
        "libs/leagues/features/leagues-join/src/index.ts"
      ],
      "@f1-predictions/leagues-list": [
        "libs/leagues/features/leagues-list/src/index.ts"
      ],
      "@f1-predictions/leagues-store": ["libs/leagues/store/src/index.ts"],
      "@f1-predictions/models": ["libs/shared/data-access/models/src/index.ts"],
      "@f1-predictions/players-store": ["libs/players/store/src/index.ts"],
      "@f1-predictions/predictions-store": [
        "libs/predictions/store/src/index.ts"
      ],
      "@f1-predictions/predictions-team-list": [
        "libs/predictions/feature/predictions-team-list/src/index.ts"
      ],
      "@f1-predictions/race-store": ["libs/races/store/src/index.ts"],
      "@f1-predictions/results-list": [
        "libs/results/feature/results-list/src/index.ts"
      ],
      "@f1-predictions/results-store": ["libs/results/store/src/index.ts"],
      "@f1-predictions/shell/feature/web-shell": [
        "libs/shell/feature/web-shell/src/index.ts"
      ],
      "@f1-predictions/signup": ["libs/auth/feature/signup/src/index.ts"],
      "@f1-predictions/utils": ["libs/shared/utils/src/index.ts"],
      "@spartan-ng/ui-button-helm": [
        "libs/shared/ui/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/ui-card-helm": ["libs/shared/ui/ui-card-helm/src/index.ts"],
      "@spartan-ng/ui-icon-helm": ["libs/shared/ui/ui-icon-helm/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
